<script>
    // Sample data for different time periods
    let visitorsData = {
        "ALL": { today: 1024, month: 12356, year: 102354, series: [10, 15, 35, 40, 50, 65, 75, 85, 90, 95, 100, 120] },
        "1M": { today: 820, month: 11234, year: 98321, series: [5, 10, 20, 30, 50, 65, 75, 85, 95, 110, 120, 130] },
        "6M": { today: 950, month: 11987, year: 101456, series: [8, 12, 25, 35, 45, 55, 65, 75, 80, 85, 90, 100] },
        "1Y": { today: 1024, month: 12356, year: 102354, series: [10, 15, 35, 40, 50, 65, 75, 85, 90, 95, 100, 120] }
    };

    // Initialize chart
    let chartOptions = {
        chart: {
            type: 'area',
            height: 300,
            animations: {
                enabled: true,
                easing: 'easeinout',
                speed: 800
            }
        },
        series: [{ name: 'Visitors', data: visitorsData["1Y"].series }],
        colors: ['#3b82f6'],
        stroke: { curve: 'smooth', width: 2 },
        fill: { type: 'gradient', gradient: { shade: 'light', type: "vertical", stops: [0, 100] } },
        xaxis: { categories: ['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec'] },
        yaxis: { labels: { formatter: (val) => val } }
    };

    let visitorsChart = new ApexCharts(document.querySelector("#visitors-chart"), chartOptions);
    visitorsChart.render();

    // Update chart and data on button click
    document.querySelectorAll(".filter-btn").forEach(btn => {
        btn.addEventListener("click", function () {
            document.querySelectorAll(".filter-btn").forEach(b => b.classList.remove("active"));
            this.classList.add("active");
            let period = this.getAttribute("data-period");

            // Update numbers
            document.getElementById("todayVisitors").innerText = visitorsData[period].today;
            document.getElementById("monthVisitors").innerText = visitorsData[period].month;
            document.getElementById("yearVisitors").innerText = visitorsData[period].year;

            // Update chart
            visitorsChart.updateSeries([{ data: visitorsData[period].series }]);
        });
    });
</script>
<script>
    function createRadialChart(element, value, color) {
        var options = {
            chart: {
                type: 'radialBar',
                height: 100,
                animations: {
                    enabled: true,
                    easing: 'easeinout',
                    speed: 1000
                }
            },
            series: [value],
            colors: [color],
            plotOptions: {
                radialBar: {
                    hollow: {
                        size: '50%'
                    },
                    track: {
                        background: '#e9ecef'
                    },
                    dataLabels: {
                        show: false
                    }
                }
            }
        };
        var chart = new ApexCharts(document.querySelector(element), options);
        chart.render();
    }

    // Initialize Charts
    createRadialChart("#radialchart-1", 37, "#3b82f6"); // Blue
    createRadialChart("#radialchart-2", 72, "#22c55e"); // Green
    createRadialChart("#radialchart-3", 54, "#ef4444"); // Red
</script>

